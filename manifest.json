{
	"manifest_version": 2,
	"name": "Torrent With Filmweb Chrome Extension",
	"version": "0.1.0",
	"description": "Torrent With Filmweb Chrome Extension",
	"web_accessible_resources": ["ajax_loading_small.gif"],
	
	"background": {
    	"scripts":  ["js/storage.js", "js/cache.js", "background.js" ]
	},
  
	"options_page": "options.html",
  
	"permissions": [
		"storage",
	    "webRequest",
	    "webRequestBlocking",
	    "<all_urls>"
	],
	
	"content_scripts": [ {
		"all_frames": true,
		"js": ["lib/jquery.min-1.7.2.js", "lib/jquery.ajaxq-0.0.1.js", "js/storage.js", "js/cache.js", "js/misc.js", "js/filmweb.js", "js/isoHunt.js", "js/pirateBay.js", "js/main.js" ],
		"matches": [
			"http://thepiratebay.se/*",
			"http://isohunt.com/*",
			"http://www.filmweb.pl/*",
			"http://www.imdb.com/*",
			"http://ia.media-imdb.com/images/**" ]
	}]
}